# Freepik API Configuration
FREEPIK_API_KEY=your_freepik_api_key_here
FREEPIK_WEBHOOK_SECRET=your_webhook_secret_here
FREEPIK_WEBHOOK_URL=https://your-domain.com/webhook

# LLM Configuration (choose one)
OPENAI_API_KEY=your_openai_key_here
ANTHROPIC_API_KEY=your_claude_key_here
LLM_MODEL=gpt-4

# Environment Settings
ENVIRONMENT=development
DEBUG=True

# Database Configuration
DATABASE_URL=postgresql://user:password@localhost:5432/freepik_orchestrator

# Optional: Redis for caching
REDIS_URL=redis://localhost:6379/0

# Optional: Stripe for billing
STRIPE_PUBLIC_KEY=pk_test_...
STRIPE_SECRET_KEY=sk_test_...

# Security
SECRET_KEY=your-super-secret-key-here
JWT_SECRET_KEY=jwt-secret-key-here

# Logging
LOG_LEVEL=INFO
